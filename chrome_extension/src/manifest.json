{
  "name": "chrome_extension",
  "description": "Chrome Extension",
  "version": "0.0.5",
  "manifest_version": 2,
  "author": "Andrei McMillan <amcmillan@yieldbot.com>",
  "homepage_url": "https://github.com/amcmillan01/chrome_extension",
  "icons": {
    "16": "logo-small.png",
    "32": "logo.png",
    "48": "logo.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "notifications",
    "contextMenus"
  ],
  "background": {
    "page": "background.index.html"
  },
  "devtools_page": "devtools.init.html",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content_script.index.js",
        "lib/jquery-1.11.3.min.js",
        "lib/lodash.min.js"
      ],
      "css": []
    }
  ],
  "web_accessible_resources": [],
  "browser_action": {
    "default_icon": {
      "19": "logo.png"
    },
    "default_title": "",
    "default_popup": "browser_action.index.html"
  }
}